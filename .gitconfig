[core]
    excludesFile = ~/.dotfiles/.gitignore
[init]
    defaultBranch = main
[push]
    autoSetupRemote = true
[gpg]
   format = ssh
[user]
   name = "Anonymous"
   email = "<>"
[includeIf "gitdir:~/Documents/Code/WiDuMu/*"]
   path = ~/.dotfiles/gitconfig/WiDuMu
[includeIf "gitdir:~/Documents/Code/WiDuMu/"]
   path = ~/.dotfiles/gitconfig/WiDuMu
[includeIf "gitdir:~/Documents/WiDuMu/*"]
   path = ~/.dotfiles/gitconfig/WiDuMu
[includeIf "gitdir:~/.cfg"]
   path = ~/.dotfiles/gitconfig/WiDuMu
[alias]
    s = status
    l = log --decorate --date=short
    c = commit
    b = branch
    branch-delete = branch -D
    cl = clone
    co = checkout
    cob = checkout -b
    d = diff
    f = fetch
    update = add -u
    up = add -u
    u = add -u
    save = commit -a
    sa = commit -a
    amend = commit --amend --no-edit
    amend-msg = commit --amend
    i = init
    pl = pull
    ps = push
    undo = reset HEAD~1 --mixed
    undo-hard = reset --hard
